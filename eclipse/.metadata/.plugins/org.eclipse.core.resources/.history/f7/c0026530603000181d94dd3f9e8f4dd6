package com.flippity.ei.common.tiles;

import com.flippity.ei.common.tiles.atomic.TEElectromagnet;
import com.flippity.ei.common.tiles.atomic.TEPlasma;
import com.flippity.ei.common.tiles.atomic.TETurbine;
import com.flippity.ei.common.tiles.base.batteries.TET1Battery;
import com.flippity.ei.common.tiles.base.cables.TECable;
import com.flippity.ei.common.tiles.base.cables.copper.TECableCopperInsulated;
import com.flippity.ei.common.tiles.base.cables.copper.TECableCopperUninsulated;
import com.flippity.ei.common.tiles.base.cables.superconductor.TECableSuperconductorInsulated;
import com.flippity.ei.common.tiles.base.cables.superconductor.TECableSuperconductorUninsulated;
import com.flippity.ei.common.tiles.base.machines.TEBoreDrillCrank;
import com.flippity.ei.common.tiles.base.machines.TECoalGenerator;

import cpw.mods.fml.common.registry.GameRegistry;

public class TileEntityRegistry {
	public static void registerTE() {
		GameRegistry.registerTileEntity(TECableCopperUninsulated.class, "CopperCableUninsulated");
		GameRegistry.registerTileEntity(TECableCopperInsulated.class, "CopperCableInsulated");
		GameRegistry.registerTileEntity(TECableSuperconductorUninsulated.class, "SuperconductorCableUninsulated");
		GameRegistry.registerTileEntity(TECableSuperconductorInsulated.class, "SuperconductorCableInsulated");
		GameRegistry.registerTileEntity(TEPlasma.class, "Plasma");
		GameRegistry.registerTileEntity(TEElectromagnet.class, "Electromagnet");
		GameRegistry.registerTileEntity(TETurbine.class, "Turbine");
		GameRegistry.registerTileEntity(TEBoreDrillCrank.class, "BoreDrillCrank");
		GameRegistry.registerTileEntity(TECable.class, "Cable");
		GameRegistry.registerTileEntity(TET1Battery.class, "BatteryT1");
		
		
		GameRegistry.registerTileEntity(TECoalGenerator.class, "CoalGenerator");
	}
}

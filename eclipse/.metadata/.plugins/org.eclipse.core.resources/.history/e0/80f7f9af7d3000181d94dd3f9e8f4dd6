package com.flippity.ei.client.gui;

import net.minecraft.util.ResourceLocation;

public class GuiElementPowerHorizontalLarge extends GuiElement
{
	private static final int TEXTURE_WIDTH = 162;
	private static final int TEXTURE_HEIGHT = 32;
	
	private static final int PROGRESS_START_X = 100;
	private static final int PROGRESS_START_Y = 58;
	
	protected static ResourceLocation texture = new ResourceLocation("ei" + ":" + "textures/gui/powerHorizontalLarge.png");
	
	public GuiElementPowerHorizontalLarge(int x, int y)
	{
		super(texture, x, y, PROGRESS_START_X, PROGRESS_START_Y, TEXTURE_WIDTH, TEXTURE_HEIGHT);
	}
	
}